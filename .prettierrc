{
	"arrowParens": "avoid",
	"endOfLine": "auto",
	"printWidth": 120,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "all",
	"useTabs": true,

	"overrides": [
		{
			"files": ".prettierrc",
			"options": { "parser": "json" }
		},
		{
			"files": ".json",
			"options": {
				"trailingComma": "none"
			}
		}
	]
}
