{
	"editor.formatOnSave": true,
  	"editor.formatOnPaste": true,
	"explorer.compactFolders": false,
	"typescript.tsdk": "node_modules/typescript/lib",
	"files.eol": "\n",
	"[typescript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint",
		"editor.formatOnSave": true
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint",
		"editor.formatOnSave": true
	},
	"eslint.run": "onType",
	"eslint.format.enable": true,
	"cSpell.words": [
		"getmetatable",
		"hoarcekat",
		"rbxassetid",
		"rbxts",
		"rbxtsc",
		"screengui",
		"setmetatable",
		"tsbuildinfo"
	]
}
